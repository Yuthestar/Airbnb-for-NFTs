  <script>

document.addEventListener("DOMContentLoaded", () => {

  const myListings = document.getElementById("myListings");

  let saved = JSON.parse(localStorage.getItem("nfts")) || [];

 

  function renderCard(nft, parent) {

    const wrapper = document.createElement("div");

    wrapper.className = "card-wrapper";

    wrapper.innerHTML = `

      <div class="card-inner">

        <img src="${nft.img}" alt="${nft.title}"

             onerror="this.src='https://via.placeholder.com/400x300?text=No+Image'">

        <h3>${nft.title}</h3>

        <p class="desc">${nft.desc}</p>

        <p class="price">${nft.price} ETH / day</p>

      </div>

    `;

    parent.appendChild(wrapper);

  }

 

  // show only NFTs listed by you

  saved.forEach(nft => {

    if (nft.owner === "You") {

      renderCard(nft, myListings);

    }

  });

});

</script>

 

</body>

</html>

 

 

 

 

 

 

 

